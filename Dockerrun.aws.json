{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions":[
    {
      "name": "client",
      "image": "olizet/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "olizet/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "olizet/multi-worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "olizet/multi-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "conainerPort":80
        }
      ],
      "links": ["client","server"],
      "memory": 128
    }
  ]
}
